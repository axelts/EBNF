%% output
  EBNF Grammar's Grammar.
  Checking a grammar for 'sum'.
%% grammar
  grammar: { rule };
  rule:    Token ':' alt ';';
  alt:     seq [{ '|' seq }];
  seq:     { lit | ref | opt | some | parens };
  lit:     Lit;
  ref:     Token;
  opt:     '[' alt ']';
  some:    '{' alt '}';
  parens:  '(' alt ')';
%% tokens
  EBNF.Grammar.terminals
%% program
  sum: Number [{ ( '+' | '-' ) Number }];
